<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="skyline.skyline1">

    <uses-permission android:name="android.permission.MANAGE_DOCUMENTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".recipeViewPage"
            android:label="@string/title_activity_recipe_view_page"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="skyline.skyline1.MainActivity" />
        </activity>
        <activity
            android:name=".recipe_add_form"
            android:label="@string/title_activity_recipe_add_form"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="skyline.skyline1.MainActivity" />
        </activity>
        <activity
            android:name="skyline.skyline1.act_add_Ingredient"
            android:label="@string/title_activity_add_Ingredient"
            android:parentActivityName=".Ingredient_List">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="skyline.skyline1.MainActivity" />
        </activity>
        <activity
            android:name="skyline.skyline1.act_add_Country"
            android:label="@string/title_activity_add__origin"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="skyline.skyline1.Country_List" />
        </activity>
        <activity
            android:name="skyline.skyline1.act_add_Category"
            android:label="@string/title_activity_add__category"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="skyline.skyline1.act_category_List" />
        </activity>
        <activity
            android:name=".RecipeView"
            android:label=""
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="skyline.skyline1.MainActivity" />
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".search_activity"
            android:label="@string/title_activity_search_activity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="skyline.skyline1.MainActivity" />
        </activity>
        <activity
            android:name="skyline.skyline1.help_page_act"
            android:label="@string/title_activity_help_page"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="skyline.skyline1.MainActivity" />
        </activity>
        <activity
            android:name=".Ingredient_List"
            android:label="@string/title_activity_ingredient__list"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Country_List"
            android:label="@string/title_activity_origin__list"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="MainActivity" />
        </activity>
        <activity
            android:name=".act_category_List"
            android:label="@string/title_activity_category__list"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="MainActivity" />
        </activity>
        <activity android:name=".Change_recipe"
            android:theme="@style/AppTheme.NoActionBar">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="RecipeView" />
        </activity>
    </application>

</manifest>